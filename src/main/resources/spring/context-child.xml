<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
  <bean id="song1" class="com.geo.source.spring_simple.example2_3_chapter.ApplicationContextNested.Song"
        p:title-ref="parentTitle"/>
  <bean id="song2" class="com.geo.source.spring_simple.example2_3_chapter.ApplicationContextNested.Song"
        p:title-ref="childTitle"/>
  <bean id="song3" class="com.geo.source.spring_simple.example2_3_chapter.ApplicationContextNested.Song">
    <property name="title">
      <ref parent="childTitleParent"/>
    </property>
  </bean>
  <bean id="childTitle" class="java.lang.String" c:_0="No Such Thing"/><!--参数常量-->
</beans>