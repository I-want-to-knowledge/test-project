<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
  <bean id="lyricHolder" class="com.geo.source.spring_simple.example2_3_chapter.MapPropsSetList.LyricHolder"/>
  <bean id="collectionInjection" class="com.geo.source.spring_simple.example2_3_chapter.MapPropsSetList.CollectionInjection">
    <property name="map">
      <map>
        <entry key="someValue" value="It's a Friday, we finally made it"/>
        <entry key="someBean" value-ref="lyricHolder"/>
      </map>
    </property>
    <property name="properties">
      <props><!--只能赋值字符串-->
        <prop key="firstName">John</prop>
        <prop key="secondName">Mayer</prop>
      </props>
    </property>
    <property name="set">
      <set>
        <value>I can't believe I get to see your face</value>
        <ref bean="lyricHolder"/>
      </set>
    </property>
    <property name="list">
      <list>
        <value>You've been working and I've been waiting</value>
        <ref bean="lyricHolder"/>
      </list>
    </property>
  </bean>

</beans>